<?xml version="1.0" encoding="utf-8"?>

<odoo>
    <record id="view_production_lot_form" model="ir.ui.view">
        <field name="name">stock.lot</field>
        <field name="model">stock.lot</field>
        <field name="inherit_id" ref="stock.view_production_lot_form" />
        <field name="arch" type="xml">
            <xpath expr="//group/group" position="inside">
                <field name="owner" string="Owner" />
                <field name="formated_label" string="Formated Label" widget="CopyClipboardChar" />
                <field name="sku" string="SKU" />
                <field name="equipment_number" string="Equipment Number" />
                <field name="expire" string="Expiration Date" />
            </xpath>
        </field>
    </record>
</odoo>